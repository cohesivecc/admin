<%= simple_form_for(@object, url: sessions_path, html: { id: 'object-form' }) do |f| %>

	<%= f.input :email %>
	<%= f.input :password %>

	<button type="submit" class="waves-effect waves-light btn">Let's Go!</button>

<% end %>
<%= link_to('Forgot your password?', [:forgot_password, :sessions]) %>
